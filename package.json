    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitest/ui": "^0.1.17",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.6.0",
